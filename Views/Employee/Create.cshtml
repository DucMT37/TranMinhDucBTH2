@model TranMinhDucBTH2.Models.Employee
<h1>Tao moi employee</h1>
<hr>
<div class ="row">
    <div class="col-md-4">
        <from asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="EmployeeID" class="control-label"></label>
                <input asp-for="EmployeeID" class="form-control">
                <span asp-validation="EmployeeID" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="EmployeeName" class="control-label"></label>
                <input asp-for="EmployeeName" class="form-control">
                <span asp-validation="EmployeeName" class="text-danger"></span>
            </div>
            <br>
            <div class="form-group">
                <input class="btn btn-primary" type="submit" value="Save">
                <a class="btn btn-success" asp-action="Index">Danh Sach Employee</a>
            </div>
        </from>
    </div>
</div>